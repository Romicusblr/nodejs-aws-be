    "http-errors": "^1.8.0",
    "knex": "^0.95.10",
    "pg": "^8.7.1",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@kristiandupont/recase": "^1.1.1",
    "@serverless/typescript": "^2.55.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.14.25",
    "jest": "^27.1.0",
    "json-schema-to-ts": "^1.5.0",
    "kanel": "^2.3.1",
    "serverless": "^2.23.0",
    "serverless-webpack": "^5.3.5",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^5.20.2",
    "webpack-node-externals": "^2.5.2"
