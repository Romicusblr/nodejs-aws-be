    "migrate:create": "knex migrate:make ${NAME:-rs} -x ts",
    "seed:create": "knex seed:make ${NAME:-rs}",
    "seed:run": "knex seed:run"
  },
  "dependencies": {
    "@middy/core": "^1.5.2",
    "@middy/http-error-handler": "^2.5.1",
