{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "jest",
    "ts": "tsc",
    "deploy": "sls deploy",
    "build:models": "kanel",
    "migrate:down": "knex migrate:down",
    "migrate:up": "knex migrate:up",
    "migrate:up:all": "knex migrate:latest",
